                        <li id="Financial">
                            <a class="d-flex align-items-center" href="@Url.Action("GPACalculator","Course")"><i data-feather="circle"></i><span class="menu-item text-truncate" data-i18n="Financial">GPA Simulation</span></a>
                        </li>
                    </ul>
                </li>


            </ul>
        </div>
